import javax.swing.JOptionPane;

public class Exercicio1 {

    public static void main(String[] args) {
       soma(15,79); 
       System.out.println("A soma dos números é "+soma1(4,6));
    }
    
    public static void soma(float n1, float n2){
        JOptionPane.showMessageDialog(null, "A soma dos números é "+
                (n1+n2),"Função soma",JOptionPane.INFORMATION_MESSAGE);
    }

    public static float soma1(float n1, float n2){
        return(n1+n2);
    }
}
